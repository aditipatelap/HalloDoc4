<div class="container">    <div class="d-flex justify-content-between mt-5">        <div class="d-flex  ">            <h4 class="m-0 pt-2  "> My Profile </h4>        </div>        <button class="btn-back btn btn-outline-info  bi bi-chevron-left" onclick="location.href='@Url.Action("AdminHeader", "Admin")'">            Back        </button>    </div>    <div class="shadow-lg rounded mt-3 py-3">        <div class="px-3">            <h5 class="d-flex justify-content-start ">                Account Information            </h5>            <div class="row g-2 mt-2">                <div class="col-md">                    <div class="form-floating">                        <input type="text" class="form-control" id="floatingInputGrid" placeholder="User Name" disabled>                        <label for="floatingInputGrid" class="text-secondary">User Name </label>                    </div>                </div>                <div class="col-md">                    <div class="form-floating">                        <input type="password" class="form-control" id="floatingInputGrid" placeholder="Password">                        <label for="floatingInputGrid" class="text-secondary">Password</label>                    </div>                </div>            </div>            <div class="row g-2 mt-2 mb-4 ">                <div class="col-md">                    <div class="form-floating">                        <select class="form-select dropdown-toggle" id="floatingSelect">                            <option selected>Active</option>                            <option value="1">One</option>                            <option value="2">Two</option>                            <option value="3">Three</option>                        </select>                        <label for="floatingSelect" class="dropdown-toggle">Status</label>                    </div>                </div>                <div class="col-md ">                    <div class="form-floating">                        <select class="form-select dropdown-toggle" id="floatingSelect">                            <option selected>Master Admin</option>                            <option value="1">One</option>                            <option value="2">Two</option>                            <option value="3">Three</option>                        </select>                        <label for="floatingSelect" class="dropdown-toggle ps-4">Role</label>                    </div>                </div>            </div>            <div class="row g-2 mt-2 mb-4 ">                <div class="d-flex justify-content-end">                    <button class="btn-back btn btn-outline-info">                        Reset Password                    </button>                </div>            </div>            <h5 class="d-flex justify-content-start ">                Administrator Information            </h5>            <div class="row g-2 mt-2">                <div class="col-md">                    <div class="form-floating">                        <input type="text" class="form-control" id="floatingInputGrid" placeholder="First name" disabled>                        <label for="floatingInputGrid" class="text-secondary">First Name</label>                    </div>                </div>                <div class="col-md">                    <div class="form-floating">                        <input type="text" class="form-control" id="floatingInputGrid" placeholder="Last name" disabled>                        <label for="floatingInputGrid" class="text-secondary">Last Name</label>                    </div>                </div>            </div>            <div class="row g-2 mt-2 mb-4">                <div class="col-md">                    <div class="form-floating">                        <input type="email" class=" form-control" id="Email" placeholder="Email" disabled>                        <label for="floatingInputGrid" class="text-secondary">Email</label>                    </div>                </div>                <div class="col-md">                    <div class="form-floating">                        <input type="email" class=" form-control" placeholder="Confirm Email" disabled>                        <label for="floatingInputGrid" class="text-secondary">Confirm Email</label>                    </div>                </div>            </div>            <div class="row g-2 mt-2 mb-4">                <div class="col-md">                    <div class="form-floating">                        <input type="tel" class="form-control p-3 ps-5" placeholder="(201) 555-0123" disabled>                    </div>                </div>                <div class="col-md">                    <input type="checkbox" class="me-1" /> <span class="me-4">District of columbia </span>                    <input type="checkbox" class="me-1" /> <span class="me-4">New York</span>                    <input type="checkbox" class="me-1" /> <span class="me-4">Maryland </span>                    <input type="checkbox" class="me-1" /> <span class="me-4">vilginia </span>                </div>            </div>            <div class="row g-2 mt-2 mb-4 ">                <div class="d-flex justify-content-end">                    <button class="btn-back btn btn-outline-info">                        Edit                    </button>                </div>            </div>            <h5 class="d-flex justify-content-start ">                Mailing & Billing Information            </h5>            <div class="row g-2 mt-2">                <div class="col-md">                    <div class="form-floating">                        <input type="text" class="form-control" id="floatingInputGrid" placeholder="address1 " disabled>                        <label for="floatingInputGrid" class="text-secondary">Address1 </label>                    </div>                </div>                <div class="col-md">                    <div class="form-floating">                        <input type="text" class="form-control" id="floatingInputGrid" placeholder="address2 " disabled>                        <label for="floatingInputGrid" class="text-secondary">Address2 </label>                    </div>                </div>            </div>            <div class="row g-2 mt-2">                <div class="col-md">                    <div class="form-floating">                        <input type="text" class="form-control" id="floatingInputGrid" placeholder="city " disabled>                        <label for="floatingInputGrid" class="text-secondary">City </label>                    </div>                </div>                <div class="col-md">                    <div class="form-floating">                        <select class="form-select dropdown-toggle" id="floatingSelect">                            <option selected>Maryland </option>                            <option value="1">One</option>                            <option value="2">Two</option>                            <option value="3">Three</option>                        </select>                        <label for="floatingSelect" class="dropdown-toggle ps-4">State</label>                    </div>                </div>            </div>            <div class="row g-2 mt-2">                <div class="col-md">                    <div class="form-floating">                        <input type="text" class="form-control" id="floatingInputGrid" placeholder="Zip " disabled>                        <label for="floatingInputGrid" class="text-secondary">Zip</label>                    </div>                </div>                <div class="col-md">                    <div class="form-floating">                        <input type="tel" class="form-control p-3 ps-5" placeholder="(201) 555-0123" disabled>                    </div>                </div>            </div>            <div class="row g-2 mt-2 mb-4 ">                <div class="d-flex justify-content-end">                    <button class="btn-back btn btn-outline-info">                        Edit                    </button>                </div>            </div>        </div>    </div></div>