@using DataAccess.ViewModel;@model AdminDashboard<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validation-unobtrusive/4.0.0/jquery.validate.unobtrusive.min.js" integrity="sha512-xq+Vm8jC94ynOikewaQXMEkJIOBp7iArs3IhFWSWdRT3Pq8wFz46p+ZDFAR7kHnSFf+zUv52B3prRYnbDRdgog==" crossorigin="anonymous" referrerpolicy="no-referrer"></script><script src="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/toastr.min.js" integrity="sha512-VEd+nq25CkR676O+pLBnDW09R7VQX9Mdiij052gVCp5yVH3jGtH70Ho/UUv4mJDsEdTvqRCFZg0NKGiojGnUCw==" crossorigin="anonymous" referrerpolicy="no-referrer"></script><link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/toastr.css" integrity="sha512-3pIirOrwegjM6erE5gPSwkUzO+3cTjpnV9lexlNZqvupR64iZBnOOTiiLPb9M36zpMScbmUNIcHUqKD47M719g==" crossorigin="anonymous" referrerpolicy="no-referrer" /><div class="container my-3 mt-4 d-flex justify-content-between" id="back">    <div><h4>Submit Information</h4></div>    <button type="button" class="btn btn-outline-info p-2 pe-3 button-info backbtn" >        <i class="bi bi-chevron-left i-info me-1"></i>Back    </button></div><form class="container form-group mt-2  shadow p-5" method="post" action='@Url.Action("CreateRequestDatapost", "Admin")'>    <div class="row d-flex">        <h5 class="d-flex mb-4">Patient</h5>    </div>    <div class="row">        <div class="col-lg-6 col-sm-12 form-floating mb-3">            <input type="text" class="form-control" asp-for="CreateReqquestModel.FirstName" id="firstName" placeholder="Your First Name" required>            <label for="firstName" class="floatingLabel text-secondary ms-2">First Name <span class="text-danger"> *</span> </label>            @Html.ValidationMessageFor(Model => Model.CreateReqquestModel.FirstName, "", new { @class = "text-danger" })        </div>        <div class="col-lg-6 col-sm-12 form-floating mb-3">            <input type="text" class="form-control" asp-for="CreateReqquestModel.LastName" id="lastName" placeholder="Your Last Name" required>            <label for="lastName" class="floatingLabel text-secondary ms-2">Last Name <span class="text-danger"> *</span> </label>        @Html.ValidationMessageFor(Model => Model.CreateReqquestModel.LastName, "", new { @class = "text-danger" })           </div>    </div>    <div class="row">        <div class="col-lg-6 col-sm-12 form-floating mb-3">            <input type="text" class="form-control" asp-for="CreateReqquestModel.LastName" id="lastName" placeholder="Your Last Name" required>            <label for="lastName" class="floatingLabel text-secondary ms-2">Last Name <span class="text-danger"> *</span> </label>            @Html.ValidationMessageFor(Model => Model.CreateReqquestModel.LastName, "", new { @class = "text-danger" })        </div>        <div class="col-lg-6 col-sm-12 form-floating mb-3">                           <input class="form-control p-3 ps-5 phoneNumber" id="phone" asp-for="CreateReqquestModel.PhoneNumber" type="tel" placeholder="(201)555-0123" required>            <label for="phoneno" class="floatingLabel text-secondary ms-2">Phone Number <span class="text-danger"> *</span> </label>            @Html.ValidationMessageFor(Model => Model.CreateReqquestModel.PhoneNumber, "", new { @class = "text-danger" })            </div>        </div>        <div class="col-lg-6 col-sm-12 form-floating mb-3">            <input type="email" class="form-control" placeholder="Email" asp-for="CreateReqquestModel.Email" id="Email" required>            <label for="Email" class="floatingLabel text-secondary ms-2">Email <span class="text-danger"> *</span> </label>            @Html.ValidationMessageFor(Model => Model.CreateReqquestModel.Email, "", new { @class = "text-danger" })                </div>    </div>    <div class="row">        <div class="col-lg-6 col-sm-12 form-floating mb-3">            <input type="Date" class="form-control" asp-for="CreateReqquestModel.DOB" id="dob" placeholder="Date of Birth">            <label for="dob" class="floatingLabel text-secondary ms-2">Date of Birth (Optional)</label>            <div class="invalid-feedback">Please enter your date of birth.</div>        </div>    </div>    <h5 class="my-4">Location</h5>    <div class="row ">        <div class="col-lg-6 col-sm-12 form-floating mb-3">            <input type="text" class="form-control" asp-for="CreateReqquestModel.Street" id="street" placeholder="Street" required>            <label for="street" class="floatingLabel text-secondary ms-2">Street <span class="text-danger"> *</span> </label>            @Html.ValidationMessageFor(Model => Model.CreateReqquestModel.Street, "", new { @class = "text-danger" })                   </div>        <div class="col-lg-6 col-sm-12 form-floating mb-3">            <input type="text" class="form-control" asp-for="CreateReqquestModel.City" id="city" placeholder="City" required>            <label for="city" class="floatingLabel text-secondary ms-2">City <span class="text-danger"> *</span> </label>            @Html.ValidationMessageFor(Model => Model.CreateReqquestModel.City, "", new { @class = "text-danger" })                </div>    </div>    <div class="row">        <div class="col-lg-6 col-sm-12 form-floating mb-3">            <input type="text" class="form-control" asp-for="CreateReqquestModel.State" id="state" placeholder="State" required>            <label for="state" class="floatingLabel text-secondary ms-2">State <span class="text-danger"> *</span> </label>            <div class="invalid-feedback">Please enter the state.</div>            @Html.ValidationMessageFor(Model => Model.CreateReqquestModel.State, "", new { @class = "text-danger" })               </div>        <div class=" col-lg-6 col-sm-12 form-floating mb-3">            <input type="text" class="form-control" asp-for="CreateReqquestModel.ZipCode" id="zip" placeholder="Zip Code">            <label for="zip" class="floatingLabel text-secondary ms-2">Zip Code (Optional)</label>                   </div>    </div>    <div class="row">        <div class="col-lg-6 col-sm-12 form-floating mb-3">            <input type="text" class="form-control" id="roomNumber" placeholder="Room Number #/ Suite (Optional)">            <label for="roomNumber" class="floatingLabel text-secondary ms-2">Room # (Optional)</label>        </div>        <div class="col-lg-6 col-sm-12 form-floating mb-3 mt-2">                   </div>    </div>    <h5 class="my-4">Notes</h5>    <div class="row">        <div class=" col-lg-12 form-floating mb-3">            <textarea class="form-control" asp-for="CreateReqquestModel.Notes" placeholder="Notes" id="notes" style="height: 100px"></textarea>            <label for="notes" class="floatingLabel text-secondary ms-2">Admin Notes (Optional)</label>            <div class="invalid-feedback">Please enter some notes.</div>        </div>    </div>    <div class="d-flex justify-content-end  mt-2 mb-2">        <button type="submit" class="btn btn-lg  btn-outline-info button-info" id="SaveRequest">Save</button>        <button type="reset" asp-action="Index" asp-controller="Admin" class="btn btn-lg btn-outline-info ms-2 button-info">Cancel</button>    </div></form><script>    <script>        const phoneInputField = document.querySelectorAll("input[type='tel']");    for (var i = 0; i < phoneInputField.length; i++) {        const phoneInput = window.intlTelInput(phoneInputField[i], {            utilsScript:                "https://cdnjs.cloudflare.com/ajax/libs/intl-tel-input/17.0.8/js/utils.js",        });    }    $(window).on('load', function () {        $('#myModal').modal('show');    });</script><script>    $("#city").prop('disabled', false);    $('.Verify').click(function () {        var city = $("#city").val();        $.ajax({            url: '/Admin/VerifyAddress',            type: 'GET',            data: { city: city },            success: function (data) {                if (data.success) {                    toastr.success(data.message);                    //$("#SaveRequest").prop('disabled', false);                    $("#city").prop('disabled', true);                }                else {                    toastr.error(data.message);                }            },            error: function (xhr, status, error) {                console.error(error);            }        });    });    $('.backbtn').on('click', function () {        $("#Dashboard")[0].click();    });</script>