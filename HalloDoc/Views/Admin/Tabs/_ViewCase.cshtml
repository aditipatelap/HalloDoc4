@using DataAccess.ViewModel@model AdminDashboard<div class="d-flex justify-content-between align-items-center container pb-1 mt-5">    <div class="d-flex align-self-center">        <h3 class="">New Request</h3>@*        <btn class=" @Enum.GetName(typeof(RequestTypes),Model.RequestTypeId) mx-3 align-self-center p-2 rounded-pill text-white">@Enum.GetName(typeof(RequestTypes),Model.RequestTypeId)</btn>*@    </div>    <div class=" btn d-flex justify-content-end">        <a class="backbutton btn border border-info shadow-none"> <strong class="text-info">                <i class="fa fa-angle-left pe-1 text-info"> </i> Back            </strong>        </a>    </div></div><div class="bg-white reqform p-3 container ">    <form class="bg-white sbox" method="post">        <h5><b>Patient Information</b></h5>        <p class="text-secondary">            Conformation Number<br />            <span class="text-info h3">@*@Model.ConfNo*@</span>        </p>        <div class="row">            <div class="form-group col-lg-12 mb-3">                <textarea asp-for="@Model.viewcase.Symptoms" class="form-control" rows="4" placeholder="Patient Notes" disabled></textarea>            </div>            <div class="mb-3 col-lg-6 form-floating">                <input asp-for="@Model.viewcase.FirstName" type="text" class="form-control" placeholder="First Name" disabled>                <label for="FirstName" class="form-label">First Name</label>            </div>            <div class="mb-3 col-lg-6 form-floating">                <input type="text" asp-for="@Model.viewcase.LastName" class="form-control" placeholder="last Name" disabled>                <label for="LastName" class="form-label">Last Name</label>            </div>            <div class="mb-3 col-lg-6 form-floating">                <input type="date" asp-for="@Model.viewcase.DOB" class="form-control" id="Date" disabled>                <label class="form-label" for="Date">Date of Birth</label>            </div>            <div class="mb-3 col-lg-5 form-floating phone">                <input type="tel" asp-for="@Model.viewcase.Mobile" class="form-control shadow-none" id="phone" disabled>            </div>            <div class="mb-3 col-lg-1 form-floating">                <a class=" border border-info rounded p-3 mt-1 ">                    <i class="fa fa-phone px-1 py-2 mt-3 fa-sm text-info"> </i>                </a>            </div>            <div class="mb-3 col-lg-6 form-floating">                <input type="email" asp-for="@Model.viewcase.Email" class="form-control" id="email" mailto:placeholder="abc@email.com" aria-describedby="emailHelp" disabled>                <label for="email" class="form-label">E-mail</label>                <span asp-validation-for="@Model.viewcase.Email" class="text-danger"></span>            </div>            <div class="col-lg-6">                <a id="editprofile" onclick="enable_disable()" class=" btn btn-lg btn-info text-white rounded px-4">Edit</a>                <input type="submit" id="submit" class=" btn btn-lg bg-info text-white rounded px-3" value="Save" style="display: none;">            </div>            <h5><b> Location Information</b></h5>            <div class="row">                <div class="mb-3 col-lg-6 form-floating">                    <input type="text" asp-for="@Model.viewcase.Region" class="form-control" placeholder="Region" disabled>                    <label for="Region" class="form-label">Region</label>                </div>                <div class="mb-3 col-lg-6 form-floating">                    <input type="text" asp-for="@Model.viewcase.Address" class="form-control" placeholder="Address" disabled>                    <label for="Address" class="form-label">Address</label>                </div>                @* <div class="mb-3 col-lg-1 form-floating">                <a class=" border border-info rounded p-3 mt-1 ">                <i class="fa fa-map-marker-alt px-1 py-2 mt-3 fa-lg text-info"> </i>                </a>                </div>*@                <div class="mb-3 col-lg-6 form-floating">                    <input type="text" asp-for="@Model.viewcase.Room" class="form-control" placeholder="Room" disabled>                    <label for="Room" class="form-label">Room</label>                </div>            </div>            @*<div class="row px-3 d-flex justify-content-end">                <a class="col-1 btn btn-info text-white rounded p-2 me-2"  data-bs-toggle="modal" data-bs-target=" #assigncase"> Assign</a>                <a class="col-1 btn btn-info text-white rounded p-2 me-2" asp-action="viewNotes" asp-controller="Admin"> View Notes </a>                <a class="col-1 btn btn-danger text-white rounded p-2 me-2"> Cancel </a>            </div>*@    </form></div><script>            $(".backbutton").click(function () {                        $.ajax({                url: '/Admin/GetTabs',                data: { Tabid: tabidglobal },                method: 'GET',                success: function (data) {                    // Replace the content of the btnContent div with the retrieved partial view                    $("#TabContent").html(data);                },                error: function () {                    console.error('Error fetching partial view.');                }            });        });         </script>