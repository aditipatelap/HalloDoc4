<link rel="stylesheet" href="https://unpkg.com/leaflet/dist/leaflet.css" /><style>    #map {        position: center;        height: 75vh;        z-index: 1;    }</style><div class=" container d-flex justify-content-center align-content-center flex-column">    <div class="d-flex justify-content-between mb-3 ">        <h3 class="">Provider Location</h3>       @* <div>            <a class="btn btn-outline-info  p-2 px-4 fw-semibold" asp-controller="Admin" asp-action="dashboard" role="button">                <p class="m-0">&#60; Back</p>            </a>        </div>*@    </div>    <div id="map" class="shadow p-2 "></div></div><script>    $(document).ready(function () {        var map = L.map('map').setView([22.2587, 71.1924], 8);        googleStreets = L.tileLayer('http://{s}.google.com/vt/lyrs=m&x={x}&y={y}&z={z}', {            maxZoom: 20,            subdomains: ['mt0', 'mt1', 'mt2', 'mt3']        }).addTo(map);        $.ajax({            url: 'Admin/GetLocation',            method: 'GET',            success: function (response) {                console.log(response);                response.map(function (res) {                    L.marker([res.latitude, res.longitude]).addTo(map)                        .bindPopup(res.physicianname)                })            }        });    });</script>