 <input type="text" class="form-control inputform" id="lastName" placeholder="Last name" asp-for="@Model.myProfile.LastName"required disabled>                            <label for="lastName" class="floatingLabel text-secondary">Last Name<span class="text-danger">*</span></label>                            @Html.ValidationMessageFor(Model=>Model.myProfile.LastName,"",new{@class="text-danger",id="cityValidationMessage"})                        </div>                    </div>                </div>                <div class="row g-2 mt-2 mb-4">                    <div class="col-md">                        <div class="form-floating">                            <input type="email" class=" form-control inputform" id="Email" placeholder="Email" asp-for="@Model.myProfile.Email" required disabled>                            <label for="Email" class="floatingLabel text-secondary">Email<span class="text-danger">*</span></label>                            @Html.ValidationMessageFor(Model=>Model.myProfile.Email,"",new{@class="text-danger",id="cityValidationMessage"})                        </div>                    </div>                    <div class="col-md">                        <div class="form-floating">                            <input type="email" class=" form-control inputform" id="confirmEmail" placeholder="Confirm Email" asp-for="@Model.myProfile.ConfirmEmail" required disabled>                            <label for="confirmEmail" class="floatingLabel text-secondary">Confirm Email<span class="text-danger">*</span></label>                            @Html.ValidationMessageFor(Model=>Model.myProfile.ConfirmEmail,"",new{@class="text-danger",id="cityValidationMessage"})                        </div>                    </div>                </div>                <div class="row g-2 mt-2 mb-4">                    <div class="col-md">                        <div class="form-floating">                            <input type="phone" class=" form-control inputform"  placeholder="phone" asp-for="@Model.myProfile.PhoneNumber" required disabled>                            <label for="phone" class="floatingLabel text-secondary">Phone number<span class="text-danger">*</span></label>                            @Html.ValidationMessageFor(Model=>Model.myProfile.PhoneNumber,"",new{@class="text-danger",id="cityValidationMessage"})                        </div>                                             </div>                    <div class="col-md">                        <div class="d-flex flex-wrap align-items-baseline mt-1">                            @foreach (var i in Model.Regions)
