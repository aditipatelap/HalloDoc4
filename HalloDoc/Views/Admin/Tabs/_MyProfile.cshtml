@using DataAccess.ViewModel@model AdminDashboard@Html.ValidationSummary(true);<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validation-unobtrusive/4.0.0/jquery.validate.unobtrusive.min.js" integrity="sha512-xq+Vm8jC94ynOikewaQXMEkJIOBp7iArs3IhFWSWdRT3Pq8wFz46p+ZDFAR7kHnSFf+zUv52B3prRYnbDRdgog==" crossorigin="anonymous" referrerpolicy="no-referrer"></script><div class="container">    <div class="d-flex justify-content-between mt-5">        <div class="d-flex  ">            <h4 class="m-0 pt-2"> My Profile </h4>        </div>        <button class="btn-back btn btn-outline-info  bi bi-chevron-left" onclick="location.href='@Url.Action("Index", "Admin")'">            Back        </button>    </div>    <div class="shadow-lg rounded mt-3 py-3">        <div class="px-3">            <h5 class="d-flex justify-content-start ">                Account Information            </h5>            <div class="row g-2 mt-2">                <div class="col-md">                    <div class="form-floating">                        <input type="text" class="form-control" id="floatingInputGrid" placeholder="User Name" disabled>                        <label for="floatingInputGrid" class="text-secondary">User Name </label>                    </div>                </div>                <div class="col-md">                    <div class="form-floating">                        <input type="password" class="form-control" id="floatingInputGrid" placeholder="Password">                        <label for="floatingInputGrid" class="text-secondary">Password</label>                    </div>                </div>            </div>            <div class="row g-2 mt-2 mb-4 ">                <div class="col-md">                    <div class="form-floating">                        <select class="form-select" id="floatingSelect">                            <option selected>Active</option>                            <option value="1">One</option>                            <option value="2">Two</option>                            <option value="3">Three</option>                        </select>                        <label for="floatingSelect">Status</label>                    </div>                </div>                <div class="col-md ">                    <div class="form-floating">                        <select class="form-select" id="floatingSelect">                            <option selected>Master Admin</option>                            <option value="1">One</option>                            <option value="2">Two</option>                            <option value="3">Three</option>                        </select>                        <label for="floatingSelect" class="ps-4">Role</label>                    </div>                </div>            </div>            <div class="row g-2 mt-2 mb-4 ">                <div class="d-flex justify-content-end">                    <button class="btn-back btn btn-outline-info">                        Reset Password                    </button>                </div>            </div>            <h5 class="d-flex justify-content-start ">                Administrator Information            </h5>            <form  id="infoupadateform">            <div class="row g-2 mt-2">                <div class="col-md">                    <div class="form-floating">                        <input type="text" class="form-control" id="floatingInputGrid" asp-for=myProfile.FirstName placeholder="First name" disabled required>                        <label for="floatingInputGrid" class="text-secondary">First Name</label>                        @Html.ValidationMessageFor(Model=>Model.myProfile.FirstName,"",new{@class="text-danger"})                    </div>                </div>                <div class="col-md">                    <div class="form-floating">                        <input type="text" class="form-control" id="floatingInputGrid" asp-for=myProfile.LastName placeholder="Last name" disabled required>                        <label for="floatingInputGrid" class="text-secondary">Last Name</label>                    </div>                </div>            </div>            <div class="row g-2 mt-2 mb-4">                <div class="col-md">                    <div class="form-floating">                        <input type="email" class=" form-control" id="Email" asp-for=myProfile.Email placeholder="Email" disabled>                        <label for="floatingInputGrid" class="text-secondary">Email</label>                    </div>                </div>                <div class="col-md">                    <div class="form-floating">                        <input type="email" class=" form-control" placeholder="Confirm Email" disabled>                        <label for="floatingInputGrid" class="text-secondary">Confirm Email</label>                    </div>                </div>            </div>            <div class="row g-2 mt-2 mb-4">                <div class="col-md">                    <div class="form-floating">                        <input type="tel" class="form-control p-3 ps-5" asp-for=myProfile.PhoneNumber placeholder="(201) 555-0123" disabled>                    </div>                </div>                <div class="col-md">                        @foreach (var i in Model.Regions)
                        {
                            var selected = Model.regionCheckbox.Any(x => x.RegionId == i.Regionid);
                            if (selected)
                            {                                <div class="form-check d-flex d-inline-block">                                    <input disabled class="physicianInput border border-dark mt-1 me-2 inputform" type="checkbox" name="regioncheckbox" value="@i.Regionid" checked>                                    <label class="form-check-label" for="flexCheckDefault">                                        @i.Name                                    </label>                                </div>                            }
                            else
                            {                                <div class="form-check d-flex d-inline-block">                                    <input disabled class="physicianInput border border-dark mt-1 me-2 inputform" type="checkbox" name="regioncheckbox" value="@i.Regionid">                                    <label class="form-check-label" for="flexCheckDefault">                                        @i.Name                                    </label>                                </div>                            }
                        }                </div>            </div>            <div class="row g-2 mt-2 mb-4 ">                <div class="d-flex justify-content-end">                    <button type="button" id="editBtn" onclick="editDetails()" class="btn btn-outline-info btn-lg">Edit</button>                    <button id="saveBtn"  type="submit"  class="btn btn-info btn-lg" style="display:none">                        Save                    </button>                    <button id="cancelBtn" type="button" class="btn btn-info btn-lg" style="display:none" onclick="location.href='@Url.Action("patientDashboard", "Patient_Site")'">                        Cancel                    </button>                </div>            </div>            </form>            <h5 class="d-flex justify-content-start ">                Mailing & Billing Information            </h5>            <div class="row g-2 mt-2">                <div class="col-md">                    <div class="form-floating">                        <input type="text" class="form-control" id="floatingInputGrid" asp-for=myProfile.Address1 placeholder="address1 " disabled>                        <label for="floatingInputGrid" class="text-secondary">Address1 </label>                    </div>                </div>                <div class="col-md">                    <div class="form-floating">                        <input type="text" class="form-control" id="floatingInputGrid" asp-for=myProfile.Address2 placeholder="address2 " disabled>                        <label for="floatingInputGrid" class="text-secondary">Address2 </label>                    </div>                </div>            </div>            <div class="row g-2 mt-2">                <div class="col-md">                    <div class="form-floating">                        <input type="text" class="form-control" id="floatingInputGrid" placeholder="city " disabled>                        <label for="floatingInputGrid" class="text-secondary">City </label>                    </div>                </div>                <div class="col-md">                    <div class="form-floating">                        <select class="form-select dropdown-toggle" id="floatingSelect">                            <option selected>Maryland </option>                            <option value="1">One</option>                            <option value="2">Two</option>                            <option value="3">Three</option>                        </select>                        <label for="floatingSelect" class="dropdown-toggle ps-4">State</label>                    </div>                </div>            </div>            <div class="row g-2 mt-2">                <div class="col-md">                    <div class="form-floating">                        <input type="text" class="form-control" id="floatingInputGrid" asp-for=myProfile.Zipcode placeholder="Zip " disabled>                        <label for="floatingInputGrid" class="text-secondary">Zip</label>                    </div>                </div>                <div class="col-md">                    <div class="form-floating">                        <input type="tel" class="form-control p-3 ps-5" placeholder="(201) 555-0123" disabled>                    </div>                </div>            </div>            <div class="row g-2 mt-2 mb-4 ">                <div class="d-flex justify-content-end">                    <button type="button" id="editBtn" onclick="editDetails2()" class="btn btn-outline-info btn-lg">Edit</button>                    <button id="saveBtn" type="submit" class="btn btn-info btn-lg" style="display:none">                        Save                    </button>                    <button id="cancelBtn" type="button" class="btn btn-info btn-lg" style="display:none" onclick="location.href='@Url.Action("patientDashboard", "Patient_Site")'">                        Cancel                    </button>                </div>            </div>        </div>    </div></div><script>    function editDetails() {        var inputs = document.getElementsByTagName("input");        for (var i = 0; i < inputs.length; i++) {            inputs[i].disabled = false;        }        var selects = document.getElementsByTagName("select");        for (var i = 0; i < selects.length; i++) {            selects[i].disabled = false;        }        var editBtn = document.getElementById("editBtn");        editBtn.style.display = "none";        var saveBtn = document.getElementById("saveBtn");        saveBtn.style.display = "block";        var cancelBtn = document.getElementById("cancelBtn");        cancelBtn.style.display = "block";    }    function editDetails2() {        var inputs = document.getElementsByTagName("input");        for (var i = 0; i < inputs.length; i++) {            inputs[i].disabled = false;        }        var selects = document.getElementsByTagName("select");        for (var i = 0; i < selects.length; i++) {            selects[i].disabled = false;        }        var editBtn = document.getElementById("editBtn");        editBtn.style.display = "none";        var saveBtn = document.getElementById("saveBtn");        saveBtn.style.display = "block";        var cancelBtn = document.getElementById("cancelBtn");        cancelBtn.style.display = "block";    }</script>    <script>    $('#infoupadateform').on('submit', function () {        debugger;        var data = new FormData(this);        // Get all checkboxes with name "checkbox"        //var checkboxes = document.querySelectorAll('input[name="regioncheckbox"]:checked');        //// Loop through checked checkboxes and append their values to FormData        //checkboxes.forEach(function (checkbox) {        //    data.append("regioncheckbox", checkbox.value);        //});        $.ajax({                                   url: "/Admin/PostMyProfile",            type: "Post",            data: data,                            processData: false,                                contentType: false,           success: function (data) {                console.log("Data saved successfully.");                            },            error: function (xhr, status, error) {                console.log("Error saving data: " + error + ". Status: " + status);                console.log(xhr.responseText);            }        });});</script>