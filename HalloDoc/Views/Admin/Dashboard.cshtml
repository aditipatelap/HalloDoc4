@Html.Partial("_AdminHeader")
    <div class="row gx-4 m-4 p-3">
        <div class="col-lg-2 ">
        <button type="button" class="w-100 btn  newbtn text-start">
                
            <i class="fa-solid fa-plus "></i> <span class="fw-bolder">New</span>
            <p class="fw-bold fs-3">1452</p>

        </button>
        </div>
        <div class="col-lg-2">
        <button type="button" class="w-100 btn    pendingbtn text-start ">
            <i class="fa-solid fa-user"></i> <span class="fw-bolder">Pending</span>
            <p class="fw-bold fs-3">1452</p>

        </button>

        </div>
    <div class="col-lg-2 ">
        <button type="button" class=" w-100 btn  activebtn text-start border border-success ">
            <i class="far fa-check-circle"></i> <span class="fw-bolder text-success">ACTIVE</span>
            <p class="fw-bold fs-3 text-success">1452</p>

        </button>
        </div>
    <div class="col-lg-2 ">
        <button type="button" class="w-100 btn concludebtn text-start">
            icon <span class="fw-bolder">CONCLUDE</span>
            <p class="fw-bold fs-3">1452</p>

        </button>
        </div>
    <div class="col-lg-2">
        <button type="button" class="w-100 btn  toclosebtn  text-start">
            <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-alarm-fill" viewBox="0 0 16 16">
                <path d="M6 .5a.5.5 0 0 1 .5-.5h3a.5.5 0 0 1 0 1H9v1.07a7.001 7.001 0 0 1 3.274 12.474l.601.602a.5.5 0 0 1-.707.708l-.746-.746A6.97 6.97 0 0 1 8 16a6.97 6.97 0 0 1-3.422-.892l-.746.746a.5.5 0 0 1-.707-.708l.602-.602A7.001 7.001 0 0 1 7 2.07V1h-.5A.5.5 0 0 1 6 .5m2.5 5a.5.5 0 0 0-1 0v3.362l-1.429 2.38a.5.5 0 1 0 .858.515l1.5-2.5A.5.5 0 0 0 8.5 9zM.86 5.387A2.5 2.5 0 1 1 4.387 1.86 8.04 8.04 0 0 0 .86 5.387M11.613 1.86a2.5 2.5 0 1 1 3.527 3.527 8.04 8.04 0 0 0-3.527-3.527" />
            </svg> <span class="fw-bolder">TO CLOSE</span>
            <p class="fw-bold fs-3">1452</p>

        </button>
       </div>
       <div class="col-lg-2">

        <button type="button" class="w-100 btn rounded unpaidbtn text-start">
            icon <span class="fw-bolder">UNPAID</span>
            <p class="fw-bold fs-3">1452</p>

        </button>
        </div>
    </div>


    <div class="d-flex">
        <div>
            <p>Patients</p>
        </div>

        <div class="justify-content-end">
            <button type="button" class="btn">icon<span>Send Link</span></button>
            <button type="button" class="btn">icon<span>Create Requests</span></button>
            <button type="button" class="btn">icon<span>Export</span></button>
            <button type="button" class="btn">icon<span>Export All</span></button>
            <button type="button" class="btn">icon<span>Request DTY Support</span></button>
        </div>
    </div>

    <input class="form-control mr-sm-2" type="search" placeholder="Search Patients" aria-label="Search">
    <select class="btn btn-default dropdown-toggle" data-toggle="dropdown">
        <option>All</option>
        <option>Username</option>
        <option>Email</option>
        <option>Student Code</option>
    </select>

    <button>All</button>
    <span>Patient</span>
    <span>Patient</span>
    <span>Patient</span>
    <span>Patient</span>
    <span>Patient</span>


    <table class="table table-responsive  mt-3 p-3">
        <thead class="table-secondary">
            <tr>
                <th scope="col">Name <i class="bi bi-arrow-down"></i></th>
                <th scope="col">Date Of Birth</th>

                <th scope="col">Requstor</th>
                <th scope="col">Requested Date <i class="bi bi-arrow-down"></i></th>
                <th scope="col">Phone</th>

                <th scope="col">Address</th>
                <th scope="col">Notes <i class="bi bi-arrow-down"></i></th>
                <th scope="col">Chat With</th>

                <th scope="col">Actions</th>

            </tr>
        </thead>
        <tbody>
            @if (Model != null)
            {
                @foreach (var items in Model.Dashboard)
                {


                    <tr>
                        <td>@items.createddate</td>
                        <td>@items.Status</td>


                        <td>
                            <div class="btn-group">
                                <button type="button" class="btn btn-danger dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                    Action
                                </button>
                                <div class="dropdown-menu">
                                    <a class="dropdown-item" href="#">Assign Case</a>
                                    <a class="dropdown-item" href="#">Cancel Case</a>
                                    <a class="dropdown-item" href="#">View Case</a>
                                    <a class="dropdown-item" href="#">View Notes</a>

                                    <a class="dropdown-item" href="#">Block Patient</a>
                                </div>
                            </div>
                        </td>

                    </tr>
                }
            }
        </tbody>

    </table>
</div>