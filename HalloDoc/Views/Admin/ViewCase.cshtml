@await Html.PartialAsync("_AdminHeader")<link rel="stylesheet" href="~/css/AdminSite.css"><style>    .patientbtn {        color: white;        background-color: green;        border: 0px;        border-end-end-radius: 20px;        border-start-start-radius: 20px;        font-size: 13px;        padding-top: 0px;    }</style><div class="container">    <div class="d-flex justify-content-between mt-4">        <div class="d-flex  ">            <h4 class="m-0 pt-2 fs-5 ">View Reservation </h4>            <button type="button" class="patientbtn ms-2 ">Patient</button>        </div>        <button class="btn btn-outline-info   bi bi-chevron-left" onclick="location.href='@Url.Action("Dashboard", "Admin")'">            Back        </button>    </div>    <div class="shadow rounded mt-3 py-3">        <div class="px-3">            <h5 class="d-flex justify-content-start ">                Patient Information            </h5>            <div>                <div>Confirmation Number</div>                <div class="text-info">AD1234354352323d677231</div>            </div>            <div class="form-floating my-4">                <textarea class="form-control" placeholder="Leave a comment here" id="floatingTextarea2" style="height: 100px"></textarea>                <label for="floatingTextarea2" class="text-secondary" id="label-floating">Patient Notes</label>            </div>            <hr style="border-top:dashed 1px;" />            <div class="row g-2 mt-2">                <div class="col-lg-6">                    <div class="form-floating">                        <input type="text" class="form-control" id="floatingInputGrid" placeholder="First name">                        <label for="floatingInputGrid" class="text-secondary">First Name</label>                    </div>                </div>                <div class="col-lg-6">                    <div class="form-floating">                        <input type="text" class="form-control" id="floatingInputGrid" placeholder="Last name">                        <label for="floatingInputGrid" class="text-secondary">Last Name</label>                    </div>                </div>            </div>            <div class="row g-2 mt-2 mb-4 ">                <div class="col-lg-6">                    <div class="form-floating">                        <input type="date" class="form-control" data-provide="datepicker" placeholder="Date Of Birth" value="12/05/2023">                        <label for="floatingInputGrid" class="text-secondary">Date Of Birth</label>                    </div>                </div>                <div class="col-5">                    <div class="form-floating">                        <input type="tel" class="form-control p-3 ps-5 phoneNumber" placeholder="(201) 555-0123">                    </div>                </div>                <div class="col-1 h-100 justify-content-end">                    <button class="btn-back btn btn-outline  bi bi-telephone">                    </button>                </div>            </div>            <div class="row g-2 mt-2 mb-4">                <div class="col-lg-6">                    <div class="form-floating">                        <input type="email" class="form-control" id="Email" placeholder="Email">                        <label for="floatingInputGrid" class="text-secondary">Email</label>                    </div>                </div>                <div class="col-lg-2 h-100">                    @*   <button class="btn btn-outline vh-75">                    Edit                    </button> *@                </div>            </div>            <h5 class="d-flex justify-content-start ">                Location Information            </h5>            <div class="row g-2 mb-2">                <div class="col-lg-6">                    <div class="form-floating">                        <input type="text" class="form-control" id="floatingInputGrid" placeholder="Region ">                        <label for="floatingInputGrid" class="text-secondary">Region </label>                    </div>                </div>                <div class="col-lg-4">                    <div class="form-floating">                        <input type="text" class="form-control" id="floatingInputGrid" placeholder="Business Name/Address ">                        <label for="floatingInputGrid" class="text-secondary">Business Name/Address </label>                    </div>                </div>                <div class="col-2 m-auto">                    <button class="btn btn-outline-info  bi bi-geo-alt">                    </button>                </div>            </div>            <div class="row g-2 mb-2">                <div class="form-floating col-lg-6">                    <input type="text" class="form-control" id="floatingInputGrid" placeholder="Business Name/Address ">                    <label for="floatingInputGrid" class="text-secondary ms-2">Business Name/Address </label>                </div>                <div>                </div>            </div>            <div class="d-flex justify-content-end mb-2 gap-2">                <button type="submit" class="btn btn-info text-white">Assign</button>                <button type="submit" class="btn btn-info text-white">View Notes</button>                <button type="button" class="btn btn-danger text-white">Cancel</button>            </div>        </div>    </div></div>