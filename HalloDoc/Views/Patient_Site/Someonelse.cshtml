@model DataAccess.ViewModel.familyReq@Html.Partial("_DashboardHeader")<div class="container border border-0 align-items-center mt-3" style="z-index:-1">    <div class="mb-3 mt-5 d-flex justify-content-between">        <h3 class="">Submit Information</h3>        <div>            <button type="button" class="btn btn-outline-info p-2 pe-3 button-info" onclick="location.href='@Url.Action("patientDashboard", "Patient_Site")'">                <i class="bi bi-chevron-left i-info"></i>Back            </button>        </div>        <!-- <a href="" class=" border border-info rounded p-2 pe-3 text-info text-decoration-none fw-semibold start-0">        <i class="bi bi-chevron-left"></i> <span>Back</span>        </a> -->    </div>    <form action="" class="shadow p-5" method="post" enctype="multipart/form-data">         <h4 class="fw-semibold mb-4">Patient Information</h4>        <div class="form-floating">            <textarea class="form-control my-3" placeholder="Leave a comment here" id="floatingTextarea2" style="height: 100px" asp-for="Notes"></textarea>            <label for="floatingTextarea2" class="floatingLabel input-txt">                Enter Brief Details of Symtoms                (Optional)            </label>        </div>        <div class="row gy-3">            <div class="col-xs-12 col-sm-6">                <div class="form-floating">                    <input type="text" class="form-control" placeholder="First Name" asp-for="Firstname">                    <label for="FirstName" class="floatingLabel">First Name</label>                </div>            </div>            <div class="col-xs-12 col-sm-6">                <div class="form-floating">                    <input type="text" class="form-control" placeholder="Last Name" asp-for="Lastname">                    <label for="comment" class="floatingLabel">Last Name</label>                </div>            </div>            <div class="col-xs-12 col-sm-6">                <div class="form-floating">                    <input type="date" class="form-control" placeholder="Date of Birth" asp-for="DOB">                    <label for="comment" class="floatingLabel">Date of Birth</label>                </div>            </div>        </div>        <h4 class="fw-semibold my-4 mt-5">Patient Conatact Information</h4>        <div class="row gy-3">            <div class="col-xs-12 col-sm-6">                <div class="form-floating">                    <input type="email" class="form-control" placeholder="Email" id="Email" asp-for="Email">                    <label for="Email" class="floatingLabel">Email</label>                </div>            </div>            <div class="col-xs-12 col-sm-6">                <div class="form-floating">                    <input type="tel" class="form-control p-3 ps-5" id="phoneNumber" placeholder="(201) 555-0123" asp-for="Phonenumber">                </div>            </div>        </div>        <h4 class="fw-semibold my-4 mt-5">Patient Location</h4>        <div class="row gy-3">            <div class="col-xs-12 col-sm-6">                <div class="form-floating">                    <input type="text" class="form-control" placeholder="Street" asp-for="Street">                    <label for="comment" class="floatingLabel">Street</label>                </div>            </div>            <div class="col-xs-12 col-sm-6">                <div class="form-floating">                    <input type="text" class="form-control" placeholder="City" asp-for="City">                    <label for="comment" class="floatingLabel">City</label>                </div>            </div>            <div class="col-xs-12 col-sm-6">                <div class="form-floating">                    <input type="text" class="form-control" placeholder="State" asp-for="State">                    <label for="comment" class="floatingLabel">State</label>                </div>            </div>            <div class="col-xs-12 col-sm-6">                <div class="form-floating">                    <input type="number" class="form-control" placeholder="Zip Code" asp-for="Zipcode">                    <label for="comment" class="floatingLabel">Zip Code</label>                </div>            </div>            <div class="col-xs-12 col-sm-6">                <div class="form-floating">                    <input type="number" class="form-control" placeholder="Room #/Suite (Optional)">                    <label for="comment" class="floatingLabel">Room #/Suite (Optional)</label>                </div>            </div>            <div class="col-xs-12 col-sm-6">                <button type="button" class=" btn btn-outline-info p-3 d-inline d-xl-none d-lg-none">                    <i class="bi bi-geo-alt text-info me-1 i-info "></i>                </button>                <button type="button" class="btn btn-outline-info p-3 d-none d-xl-inline d-lg-inline i-info">                    <i class="bi bi-geo-alt text-info me-1 i-info"></i>Map                </button>            </div>        </div>        <h4 class="fw-semibold my-4 mt-5">Relation</h4>        <div class="row gy-3">            <div class="col-xs-12 col-sm-6">                <div class="form-floating">                    <input type="text" class="form-control" placeholder="Relation With Patient">                    <label for="comment" class="floatingLabel">Relation With Patient</label>                </div>            </div>            <div class="col-xs-12 col-sm-6">            </div>        </div>        <h4 class="fw-semibold my-4 mt-5">(Optional) Upload Photo or Document</h4>        <div class="d-flex">            <div class="file-name input-group-text border-top border-bottom -border-start border-rounded  col-lg-11 col-sm-10 col-xs-7 col-8" id="file-name">Select File</div>            <div class="file-upload-container col-lg-1 col-sm-2 mx-0 col-xs-5 col-4 px-0">                <label for="formFile" class="input-group-text bg-primary text-white">                    <i class="fa fa-cloud-arrow-up "></i>&nbsp;Upload                </label>                <input class="d-none" type="file" id="formFile" name="file">            </div>        </div>        <div class="d-flex justify-content-end my-4 mb-5">            <button type="submit" class="btn btn-info btn-lg p-2 px-4 me-3 info-button">Submit</button>            <button type="button" class="btn btn-outline-info btn-lg p-2 px-4 button-info">Cancel</button>        </div>    </form></div>@Html.Partial("_Footer")