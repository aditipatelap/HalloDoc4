@model DataAccess.ViewModel.patientReq@Html.Partial("_DashboardHeader")<style>    .defaulthidden{                display: none;       }</style><div class="container border border-0 align-items-center mt-3" style="z-index:-1">    <div class="mb-3 mt-5 d-flex justify-content-between">        <h3 class="">Submit Information</h3>        <div>            <button type="button" class="btn btn-outline-info p-2 pe-3 button-info" onclick="location.href='@Url.Action("patientDashboard", "Patient_site")'">                <i class="bi bi-chevron-left i-info"></i>Back            </button>        </div>        <!-- <a href="" class=" border border-info rounded p-2 pe-3 text-info text-decoration-none fw-semibold start-0">        <i class="bi bi-chevron-left"></i> <span>Back</span>        </a> -->    </div>    <form action="" class="shadow p-5" method="Post" enctype="multipart/form-data">        <h4 class="fw-semibold mb-4">Patient Information</h4>        <div class="form-floating">            <textarea class="form-control my-3" placeholder="Leave a comment here" id="floatingTextarea2" style="height: 100px"></textarea>            <label for="floatingTextarea2" class="floatingLabel input-txt">                Enter Brief Details of Symtoms                (Optional)            </label>        </div>        <div class="row gy-3">            <div class="col-xs-12 col-sm-6">                <div class="form-floating">                    <input type="text" class="form-control inputf" asp-for="Firstname" placeholder="First Name" >                    <label for="comment" class="floatingLabel text-dark">First Name<span class="text-danger">*</span></label>                    <span asp-validation-for=" Firstname" id="Errorinfo" class="defaulthidden"></span>                </div>            </div>            <div class="col-xs-12 col-sm-6">                <div class="form-floating">                    <input type="text" class="form-control" placeholder="Last Name" asp-for="Lastname" >                    <label for="comment" class="floatingLabel">Last Name<span class="text-danger">*</span></label>                    <span asp-validation-for="Lastname" id="Errorinfo" class="defaulthidden"></span>                </div>            </div>            <div class="col-xs-12 col-sm-6">                <div class="form-floating">                    <input type="date" class="form-control" placeholder="Date of Birth" id="dob" asp-for="DOB">                    <label for="comment" class="floatingLabel">Date of Birth</label>                    <span asp-validation-for="DOB" id="Errorinfo" class="defaulthidden"></span>                </div>            </div>        </div>        <h4 class="fw-semibold my-4 mt-5">Patient Conatact Information</h4>        <div class="row gy-3">            <div class="col-xs-12 col-sm-6">                <div class="form-floating">                    <input type="email" class="form-control" placeholder="Email" id="Email" asp-for="Email" disabled>                    <label for="Email" class="floatingLabel">Email<span class="text-danger">*</span></label>                    <span asp-validation-for="Email" id="Errorinfo" class="defaulthidden"></span>                </div>            </div>            <div class="col-xs-12 col-sm-6">                <div class="form-floating">                    <input type="tel" class="form-control p-3 ps-5" id="phoneNumber" placeholder="(201) 555-0123" asp-for="Phonenumber">                    <span asp-validation-for="Phonenumber" id="Errorinfo" class="defaulthidden"></span>                </div>            </div>        </div>        <h4 class="fw-semibold my-4 mt-5">Patient Location</h4>        <div class="row gy-3">            <div class="col-xs-12 col-sm-6">                <div class="form-floating">                    <input type="text" class="form-control" placeholder="Street" asp-for="Street">                    <label for="comment" class="floatingLabel">Street</label>                </div>            </div>            <div class="col-xs-12 col-sm-6">                <div class="form-floating">                    <input type="text" class="form-control" placeholder="City" asp-for="City">                    <label for="comment" class="floatingLabel">City</label>                </div>            </div>            <div class="col-xs-12 col-sm-6">                <div class="form-floating">                    <select class="form-select form-control inputform1" asp-for="State">                        @foreach (var i in Model.Regions)
                        {                            <option value="@i.Name">@i.Name</option>                        }                    </select>                    <label for="state" class="dropdown-toggle" asp-for="State">State<span class="text-danger">*</span></label>                </div>            </div>            <div class="col-xs-12 col-sm-6">                <div class="form-floating">                    <input type="number" class="form-control" placeholder="Zip Code" asp-for="Zipcode">                    <label for="comment" class="floatingLabel">Zip Code<span class="text-danger">*</span></label>                    <span asp-validation-for="Zipcode" id="Errorinfo" class="defaulthidden"></span>                </div>            </div>            <div class="col-xs-12 col-sm-6">                <div class="form-floating">                    <input type="number" class="form-control" placeholder="Room #/Suite (Optional)">                    <label for="comment" class="floatingLabel">Room #/Suite (Optional)</label>                </div>            </div>                    </div>        <h4 class="fw-semibold my-4 mt-5">(Optional) Upload Photo or Document</h4>                <div class="d-flex">            <div class="input-group mb-3">                <input type="file" class="form-control" id="inputGroupFile02">                <label class="input-group-text bg-primary text-white" for="inputGroupFile02">                    <i class="fa fa-cloud-arrow-up "></i>&nbsp;Upload                </label>            </div>        </div>      @*  <div class="input-group mb-3">            <input type="file" class="form-control d-none" id="formFile" name="file">            <label class="input-group-text border-0 col-12 p-0 rounded" for="formFile">                <div class="border rounded col-10 p-3 text-start" id="file-name">                    <span>Select File</span>                </div>                <div class="border uploadDiv bg-info col-2 text-light p-3">                    <i class="bi bi-cloud-arrow-up pe-1 text-white"></i>                    <span class="d-none d-xl-inline d-lg-inline ">Upload</span>                </div>            </label>        </div>*@        <div class="d-flex justify-content-end my-4 mb-5">            <button type="submit" class="btn btn-info btn-lg p-2 px-4 me-3 info-button">Submit</button>            <button type="button" class="btn btn-outline-info btn-lg p-2 px-4 button-info">Cancel</button>        </div>    </form></div><script>    document.querySelector('form').addEventListener('submit', function (event) {        var validationMessages = document.querySelectorAll('.defaulthidden');        for (var i = 0; i < validationMessages.length; i++) {            validationMessages[i].classList.remove('defaulthidden');        }    });    const phoneInputField = document.querySelectorAll("input[type='tel']");    for (var i = 0; i < phoneInputField.length; i++) {        const phoneInput = window.intlTelInput(phoneInputField[i], {            utilsScript:                "https://cdnjs.cloudflare.com/ajax/libs/intl-tel-input/17.0.8/js/utils.js",        });    }    $(window).on('load', function () {        $('#myModal').modal('show');    });    window.addEventListener('DOMContentLoaded', function () {        document.getElementById('dob').value = '';    });    $(document).ready(function () {        $('input[type=text], textarea,input[type=tel],input[type=email]').keyup(function () {            var value = $(this).val();            if (!/\S/.test(value)) {                $(this).val($.trim(value));            }        });    });</script><script>    var phone1 = document.querySelector("#phoneNumber1");    var phoneInput1 = window.intlTelInput(phone1, {        utilsScript:            "https://cdnjs.cloudflare.com/ajax/libs/intl-tel-input/17.0.8/js/utils.js",        nationalMode: false,        showSelectedDialCode: true,        autoInsertDialCode: true,        formatOnDisplay: false,    });    var phone2 = document.querySelector("#phoneNumber2");    var phoneInput2 = window.intlTelInput(phone2, {        utilsScript:            "https://cdnjs.cloudflare.com/ajax/libs/intl-tel-input/17.0.8/js/utils.js",        nationalMode: false,        showSelectedDialCode: true,        autoInsertDialCode: true,        formatOnDisplay: false,    });</script></script>